
{% extends 'base.html.twig' %}
{% dump %}

{% block body %}
    <section class="section-presentation particulier">

        <h1> {{ part.titre | upper }} </h1>
        <h2 id="BO-text-presentation-particulier"> {{ part.text }} </h2>
        <div class="presentation-global">
            <div class="presentation">
                <a href="particuliers/Evenement"><img class="photo-presentation" id="BO-photo-1-presentation-particulier" src="{{ evenement.photo | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-1-particulier"> {{ evenement.titre }} </P> </a>
            </div>
            <div class="presentation">
                <a href="particuliers/Workshop"><img class="photo-presentation" id="BO-photo-2-presentation-particulier" src="{{ workshop.photo  | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-2-particulier">{{ workshop.titre }}</P> </a>
            </div>
            <div class="presentation">
                <a href="particuliers/Studio"><img class="photo-presentation" id="BO-photo-3-presentation-particulier" src="{{ studio.photo | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-3-particulier">Studio</P> </a>
            </div>
            <div class="presentation">
                <a href="particuliers/Animaux"><img class="photo-presentation" id="BO-photo-4-presentation-particulier" src="{{ animaux.photo | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-4-particulier"> {{ animaux.titre }} </P> </a>
            </div>

            <div class="presentation">
                <a href="particuliers/Charme"><img class="photo-presentation" id="BO-photo-5-presentation-particulier" src="{{ charme.photo | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-5-particulier"> {{ charme.titre }} </P> </a>
            </div>
            <div class="presentation">
                <a href="particuliers/Lifestyle"><img class="photo-presentation" id="BO-photo-6-presentation-particulier" src="{{ lifestyle.photo | imagine_filter('squared_thumbnail') }}">
                    <P class="text-presentation" id="BO-titre-photo-6-particulier"> {{ lifestyle.titre }} </P> </a>
            </div>
        </div>
    </section>







{% endblock %}
